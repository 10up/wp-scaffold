{
  "name": "10up/plugin-scaffold",
  "description": "TenUp Plugin Scaffold",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "10up",
      "email": "info@10up.com",
      "homepage": "https://10up.com",
			"role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "TenUpPlugin\\": "includes/TenUpPlugin/"
    },
    "files": [
      "includes/TenUpPlugin/Helper/Helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5.0",
    "10up/phpcs-composer": "dev-master",
    "squizlabs/php_codesniffer": "~3.5.0"
  },
  "scripts": {
    "lint": "phpcs .",
    "lint-fix": "phpcbf ."
  }
}
