{
  "version": 1,
  "settings": {
    "custom": {
      "color": {
        "base": {
          "primary": {
            "ultralight": "#fecaca",
            "light": "#dc2626",
            "medium": "#b91c1c",
            "dark": "#991b1b"
          },
          "secondary": {
            "ultralight": "#bbf7d0",
            "light": "#16a34a",
            "medium": "#15803d",
            "dark": "#166534"
          },
          "tertiary": {
            "ultralight": "#bfdbfe",
            "light": "#2563eb",
            "medium": "#1d4ed8",
            "dark": "#1e40af"
          },
          "grey": {
            "100": "#f3f4f6",
            "300": "#d1d5db",
            "500": "#6b7280",
            "700": "#374151",
            "900": "#111827"
          },
          "dark": "#000",
          "light": "#FFF",
          "alert": "#ef4444",
          "succes": "#22c55e",
          "disabled": "var(--wp--custom--color--base--grey--300)"
        },
        "text": {
          "primary": "var(--wp--custom--color--dark)",
          "on-dark": "var(--wp--custom--color--light)",
          "link": "var(--wp--custom--color--base--primary--medium)",
          "disabled": "var(--wp--custom--color--disabled)"
        },
        "interactive": {
          "primary": {
            "default": "var(--wp--custom--color--base--primary--medium)",
            "active": "var(--wp--custom--color--base--primary--dark)"
          },
          "secondary": {
            "default": "var(--wp--custom--color--base--secondary--medium)",
            "active": "var(--wp--custom--color--base--secondary--dark)"
          },
          "tertiary": {
            "default": "var(--wp--custom--color--base--tertiary--medium)",
            "active": "var(--wp--custom--color--base--tertiary--dark)"
          }
        }
      },
      "elevation": {
        "0": "none",
        "1": "0 1px 3px rgb(0 0 0 / 12%), 0 1px 2px rgb(0 0 0 / 24%)",
        "2": "0 3px 6px rgb(0 0 0 / 16%), 0 3px 6px rgb(0 0 0 / 23%)",
        "3": "0 10px 20px rgb(0 0 0 / 19%), 0 6px 6px rgb(0 0 0 / 23%)",
        "4": "0 14px 28px rgb(0 0 0 / 25%), 0 10px 10px rgb(0 0 0 / 22%)",
        "5": "0 19px 38px rgb(0 0 0 / 30%), 0 15px 12px rgb(0 0 0 / 22%)"
      },
      "space": {
        "base": "8px",
        "half-x": "calc(var(--wp--custom--space--base) * .5)",
        "2-x": "calc(var(--wp--custom--space--base) * 2)",
        "3-x": "calc(var(--wp--custom--space--base) * 3)",
        "4-x": "calc(var(--wp--custom--space--base) * 4)",
        "5-x": "calc(var(--wp--custom--space--base) * 5)",
        "inset": {
          "squish": {
              "half-x": "2px 4px",
              "1-x": "4px 8px",
              "2-x": "8px 16px",
              "3-x": "12px 24px",
              "4-x": "16px 32px",
              "6-x": "24px 48px"
          }
        }
      },
      "font": {
        "family": {
          "base": "Arial, Helvetica, sans-serif",
          "secondary": "Times New Roman, Times, serif"
        },
        "weight": {
          "regular": "400",
          "medium": "500",
          "semibold": "600",
          "bold": "700"
        },
        "size": {
          "scale": "1.25",
          "base": "16px",
          "sm": "calc(var(--wp--custom--font--size--base) / var(--wp--custom--font--size--scale))",
          "lg": "calc(var(--wp--custom--font--size--base) * var(--wp--custom--font--size--scale))",
          "xl": "calc(var(--wp--custom--font--size--lg) * var(--wp--custom--font--size--scale))",
          "2-xl": "calc(var(--wp--custom--font--size--xl) * var(--wp--custom--font--size--scale))",
          "3-xl": "calc(var(--wp--custom--font--size--2-xl) * var(--wp--custom--font--size--scale))",
          "4-xl": "calc(var(--wp--custom--font--size--3-xl) * var(--wp--custom--font--size--scale))",
          "5-xl": "calc(var(--wp--custom--font--size--4-xl) * var(--wp--custom--font--size--scale))",
          "6-xl": "calc(var(--wp--custom--font--size--5-xl) * var(--wp--custom--font--size--scale))",
          "7-xl": "calc(var(--wp--custom--font--size--6-xl) * var(--wp--custom--font--size--scale))",
          "8-xl": "calc(var(--wp--custom--font--size--7-xl) * var(--wp--custom--font--size--scale))",
          "9-xl": "calc(var(--wp--custom--font--size--8-xl) * var(--wp--custom--font--size--scale))"
        },
        "line-height": {
          "base": "1.5",
          "tight": "1.25",
          "loose": "2"
        },
        "decoration": {
          "none": "none",
          "hover": "underline",
          "focus": "underline"
        }
      },
      "text-decoration": {
        "default": {
          "none": "none",
          "hover": "underline",
          "focus": "underline"
        },
        "interactive" : {
          "default": "none",
          "hover": "none",
          "focus": "none"
        }
      },
      "transition": {
        "timing": {
          "base": "0.2s",
          "fast": "0.1s",
          "slow": "0.4s"
        }
      },
      "border": {
        "color": {
          "default": "currentColor",
          "focus": "var(--wp--custom--color--base--primary--medium)",
          "error": "var(--wp--custom--color--base--alert)",
          "disabled": "var(--wp--custom--color--base--disabled)"
        },
        "style": {
          "default": "solid"
        },
        "width": {
          "default": "2px",
          "sm": "1px",
          "lg": "4px"
        },
        "radius": {
          "none": "none",
          "default": "4px",
          "sm": "2px",
          "lg": "8px",
          "pill": "5em"
        }
      },
      "content": {
        "width": {
          "default": "800px",
          "wide": "1000px"
        }
      }
    },
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "var(--wp--preset--color--primary--medium)"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "var(--wp--preset--color--secondary--medium)"
        },
        {
          "name": "Primary",
          "slug": "primary",
          "color": "var(--wp--preset--color--secondary--medium)"
        },
        {
          "name": "Dark",
          "slug": "dark",
          "color": "var(--wp--preset--color--secondary--dark)"
        },
        {
          "name": "Light",
          "slug": "light",
          "color": "var(--wp--preset--color--secondary--light)"
        }
      ]
    },
    "layout": {
      "contentSize": "var(--wp--preset--content--width--default)",
      "wideSize": "var(--wp--preset--content--width--wide)"
    },
    "spacing": {
      "customMargin": false,
      "customPadding": false,
      "units": []
    },
    "typography": {
      "customFontSize": false,
      "customLineHeight": false,
      "dropCap": false,
      "fontSizes": []
    },
    "blocks": {
      "core/button": {
        "border": {
          "customRadius": false
        }
      }
    }
  },
  "styles": {
    "color": {},
    "elements": {
      "h1": {},
      "h2": {},
      "h3": {},
      "h4": {},
      "h5": {},
      "h6": {},
      "link": {}
    },
    "typography": {}
  }
}
